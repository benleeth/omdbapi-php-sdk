{
    "name": "benleeth/omdb-php-sdk",
    "description": "A PHP SDK to interact with OMDb API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Leeth",
            "email": "benleeth85@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "OmdbPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OmdbPhpSdk\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
